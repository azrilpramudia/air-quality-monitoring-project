generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "mysql"
    url      = env("DATABASE_URL")
}

model SensorData {
    id          Int      @id @default(autoincrement())
    temperature Float
    humidity    Float
    tvoc        Int
    eco2        Int
    dust        Float
    aqi         Float? 
    createdAt   DateTime @default(now())
}
